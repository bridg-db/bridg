{
  "name": "usage",
  "private": true,
  "version": "1.0.0",
  "main": "src/app.js",
  "license": "MIT",
  "scripts": {
    "test:prepare": "npm run test:prepare-int && npm run test:prepare-pulse",
    "test:prepare-int": "pnpm --F bridg -- npm run prepack && npx prisma db push --schema=__tests__/__fixtures__/test.prisma;",
    "test:prepare-pulse": "pnpm --F bridg -- npm run prepack && npx prisma db push --schema=__tests__/__fixtures__/pulse-test.prisma;",
    "test": "jest --runInBand",
    "test:int": "jest --testPathPattern='^(?!.*pulse.test.ts$).*.test.ts$' --runInBand",
    "test:pulse": "jest pulse.test.ts --runInBand",
    "test:prisma-versions": "npx ts-node ./__tests__/e2e/test-prisma-versions.ts",
    "prisma:install": "pnpm i -D prisma@$VERSION && pnpm i @prisma/client@$VERSION",
    "prisma:versions": "npm view prisma versions | grep -v '-'"
  },
  "devDependencies": {
    "@jest/globals": "^29.6.2",
    "@types/jest": "27.0.3",
    "@types/node": "16.11.7",
    "bridg": "workspace:*",
    "jest": "27.4.7",
    "prisma": "5.6.0",
    "ts-jest": "27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "4.5.2"
  },
  "dependencies": {
    "@prisma/client": "5.6.0",
    "@prisma/extension-pulse": "^0.1.8"
  }
}
